server:
  port: 8083

spring:
  application:
    name: CONFIG-SERVICE
  cloud:
    config:
      server:
        jdbc:
          sql: SELECT `value` FROM `config_properties` WHERE `key` = ? AND `profile` = ? AND `label` = ?
      bootstrap: false
  datasource:
    url: jdbc:mysql://localhost:3306/db
    username: root
    password: root
  jpa:
    hibernate:
      ddl-auto: update
    show-sql: true